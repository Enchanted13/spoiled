{
  "manifest_version": 2,

  "name": "Spoiler Spoiler",
  "description": "This extension blocks content containing user-specified keywords.",
  "version": "1.0",

  "browser_action": {
    "default_title": "Spoiler Spoiler!",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
